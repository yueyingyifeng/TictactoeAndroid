<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yyyf.tictactoe">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tictactoe">

        <activity
            android:name=".RoomListActivity"
            android:label="@string/title_activity_room_list"
            android:theme="@style/Theme.Tictactoe.NoActionBar"></activity>
        <activity android:name=".InternetPlayActivity" />
        <activity android:name=".LocalPlayActivity" />
        <activity
            android:name=".WelcomeActivity"
            android:label="井字棋">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>